{
  "name": "server-mockr",
  "version": "1.0.9",
  "description": "Package to mock HTTP APIs for fast and reliable testing.",
  "main": "dist/index.js",
  "files": [
    "dist",
    "!__tests__",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.mockr.json && webpack --mode production",
    "build:dev": "tsc -p ./tsconfig.mockr.json && webpack --mode development",
    "clean": "rimraf ./dist",
    "release": "npm test && npm run clean && npm run build",
    "start": "node ./dist/test.js",
    "test": "npm run build && npm run test:lint && npm run test:unit && npm run test:prettier",
    "test:lint": "tslint -p .",
    "test:unit": "jest --runInBand",
    "test:prettier": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "test:prettier:fix": "prettier --write \"./src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boschni/server-mockr.git"
  },
  "keywords": [
    "mock",
    "HTTP",
    "server",
    "test",
    "json",
    "API",
    "e2e",
    "verify",
    "stub",
    "fake",
    "dummy"
  ],
  "author": "Niek Bosch <info@niek-bosch.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boschni/server-mockr/issues"
  },
  "homepage": "https://github.com/boschni/server-mockr#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie": "^0.4.1",
    "expect": "^26.1.0",
    "express": "^4.17.1",
    "json-pointer": "^0.6.0",
    "markdown-it": "^11.0.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "path-to-regexp": "^6.1.0",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/json-pointer": "^1.0.30",
    "@types/markdown-it": "^10.0.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^12.12.48",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^3.6.0",
    "form-data": "^3.0.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
